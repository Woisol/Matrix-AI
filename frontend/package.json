{
  "name": "matrix-ai-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node scripts/fix-monaco-editor.js && ng build",
    "publish": "pnpm run build && node scripts/clean-monaco.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "node scripts/fix-monaco-editor.js",
    "postpublish": ".\\scripts\\deploy.bat"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@ant-design/icons-angular": "^20.0.0",
    "monaco-editor": "^0.52.2",
    "ng-zorro-antd": "^20.1.0",
    "ngx-markdown": "^20.0.0",
    "ngx-monaco-editor-v2": "^20.0.0",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.0",
    "swiper": "^11.2.10",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/less": "^3.0.8",
    "@types/prismjs": "^1.26.5",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "less": "^4.4.0",
    "quicktype": "^23.2.6",
    "typescript": "~5.8.2"
  }
}